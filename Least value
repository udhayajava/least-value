import java.util.Arrays; 
import java.util.Collections; 
  
public class MinNMax 
{ 
public static void main(String[] args) 
    { 
  
         
        Integer[] INR = { 6700, 5500, 800, 9900 }; 
  
       
        int min = Collections.min(Arrays.asList(INR)); 
  
   
        int max = Collections.max(Arrays.asList(INR)); 
  
     
        System.out.println("Minimum number of array is : INR " + min); 
        System.out.println("Maximum number of array is : INR " + max); 
    } 
} 
